{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {},
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
      }
    }
  ]
}
